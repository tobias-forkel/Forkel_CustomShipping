<config>
    <modules>
        <Forkel_CustomShipping>
            <version>1.0.0</version>
        </Forkel_CustomShipping>
    </modules>
    <global>
        <models>
            <forkel_customshipping>
                <class>Forkel_CustomShipping_Model</class>
            </forkel_customshipping>
        </models>
    </global>
    <default>
        <carriers>
            <forkel_customshipping>
                <active>0</active>
                <availability>both</availability>
                <name>Custom Shipping</name>
                <title>Custom Shipping</title>
                <price>0.00</price>
                <cost>0.00</cost>
                <sallowspecific>0</sallowspecific>
                <model>forkel_customshipping/carrier_custom</model>
            </forkel_customshipping>
        </carriers>
    </default>
</config>